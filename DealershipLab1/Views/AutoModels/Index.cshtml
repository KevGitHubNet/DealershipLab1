@model IEnumerable<DealershipLab1.Models.AutoModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p><img src="https://www.google.com/search?biw=1920&bih=971&q=2015+ford+transit&tbm=isch&tbs=simg:CAQSJgnRPStgVGWBohoSCxCwjKcIGgAMCxCOrv4IGgAMITNH2mMjAQwq&sa=X&ved=0CBoQwg4oAGoVChMIkPLJ1u_7xgIV0qWICh2dBAjM#imgrc=VyGMyQV1gAVfmM%3A" alt="" />
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Make)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Model)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MPG)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.color)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MSRP)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Make)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MPG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MSRP)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
